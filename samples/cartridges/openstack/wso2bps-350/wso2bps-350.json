{
    "type": "wso2bps-350",
    "category": "framework",
    "provider": "wso2",
    "host": "bps.wso2.org",
    "displayName": "WSO2 BPS 3.5.0",
    "description": "WSO2 BPS 3.5.0 Cartridge",
    "version": "3.5.0",
    "multiTenant": false,
    "loadBalancingIPType": "public",
    "portMapping": [
        {
            "name": "mgt-http",
            "protocol": "http",
            "port": 9763,
            "proxyPort": 0
        },
        {
            "name": "mgt-https",
            "protocol": "https",
            "port": 9443,
            "proxyPort": 0
        }
    ],
    "iaasProvider": [
        {
            "type": "openstack",
            "imageId": "<REGION>/<BASE_IMAGE_ID>",
            "property": [
                {
                    "name": "instanceType",
                    "value": "<REGION>/<FLAVOUR_ID>"
                },
                {
                    "name": "keyPair",
                    "value": "<KEY_PAIR_NAME>"
                },
                {
                    "name": "securityGroups",
                    "value": "default"
                }
            ],
            "networkInterfaces": [
                {
                    "networkUuid": "<NETWORK_UUID>"
                }
            ]
        }
    ],
    "property": [
        {
            "name": "payload_parameter.CONFIG_PARAM_WORKER_HOST_NAME",
            "value": "as.wso2.com"
        },
        {
            "name": "payload_parameter.CONFIG_PARAM_CLUSTERING",
            "value": "false"
        },
        {
            "name": "payload_parameter.CONFIG_PARAM_MEMBERSHIP_SCHEME",
            "value": "stratos"
        },
        {
            "name": "payload_parameter.APPLICATION_PATH",
            "value": "/var/www"
        }
    ]
}
