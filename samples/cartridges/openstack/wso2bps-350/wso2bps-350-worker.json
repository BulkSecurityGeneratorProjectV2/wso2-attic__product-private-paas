{
    "type": "wso2bps-350-worker",
    "category": "framework",
    "provider": "wso2",
    "host": "bps.wso2.org",
    "displayName": "WSO2 BPS 3.5.0 Worker",
    "description": "WSO2 BPS 3.5.0 Worker Cartridge",
    "version": "3.5.0",
    "multiTenant": true,
    "loadBalancingIPType": "public",
    "portMapping": [
        {
            "name": "mgt-http",
            "protocol": "http",
            "port": 9763,
            "proxyPort": 0
        }
    ],
    "iaasProvider": [
        {
            "type": "kubernetes",
            "imageId": "wso2/as:5.2.1",
            "networkInterfaces": []
        }
    ],
    "iaasProvider": [
        {
            "type": "openstack",
            "imageId": "<REGION>/<BASE_IMAGE_ID>",
            "property": [
                {
                    "name": "instanceType",
                    "value": "<REGION>/<FLAVOUR_ID>"
                },
                {
                    "name": "keyPair",
                    "value": "<KEY_PAIR_NAME>"
                },
                {
                    "name": "securityGroups",
                    "value": "default"
                }
            ],
            "networkInterfaces": [
                {
                    "networkUuid": "<NETWORK_UUID>"
                }
            ]
        }
    ],
    "property": [
        {
            "name": "payload_parameter.CONFIG_PARAM_CLUSTERING",
            "value": "true"
        },
        {
            "name": "payload_parameter.LB_IP",
            "value": "<LOAD_BALANCER_IP>"
        }
    ]
}
